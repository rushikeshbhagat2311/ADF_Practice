{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-azurerushi-0401"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"delete_log_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'delete_log'"
		},
		"lsadls2rushi0401_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsadls2rushi0401'"
		},
		"lsblobrushi0401_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'lsblobrushi0401'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls2saazurerushi0401.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dailypractice.dfs.core.windows.net/"
		},
		"delete_log_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dailypractice.dfs.core.windows.net/"
		},
		"lsadls2rushi0401_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adls2saazurerushi0401.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-azurerushi-0401_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"delete_log_accountKey": {
						"value": "[parameters('delete_log_accountKey')]"
					},
					"lsadls2rushi0401_accountKey": {
						"value": "[parameters('lsadls2rushi0401_accountKey')]"
					},
					"lsblobrushi0401_connectionString": {
						"value": "[parameters('lsblobrushi0401_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"delete_log_properties_typeProperties_url": {
						"value": "[parameters('delete_log_properties_typeProperties_url')]"
					},
					"lsadls2rushi0401_properties_typeProperties_url": {
						"value": "[parameters('lsadls2rushi0401_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}